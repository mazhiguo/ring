[tree_root]
;树根节点配置
;1：只取本单位结构  2：所有单位结构  3:取本单位及所有子单位结构

;新增通讯录部门成员
from_contacts = 1

;协作区管理，增加成员
from_tribe_add_member = 1

;新建协作区，选择人员
from_tribe_add = 1

;发送系统消息
from_sys_msg = 1


[elava]
name = elava

[license]
selected = 1

[ldap]
view = 0

;为客户端提供的显示协作区图标，数据存放在频道表中
[tribe]
type = dll
url = TribeApp.dll
independency = 0

;新老协作区开关设置 0:老协作区版 1:新协作区版
switch = 0

;协作区代理服务器的ip和port
[tribeext]
ip = 172.16.0.243
port = 8085

;ms-web管理端log控制(c:\\temp\\ms-web.log.txt | /tmp/ms-web.log.txt)
[ms-web-log]
flag = 0
